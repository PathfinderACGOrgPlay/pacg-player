{
  "name": "pacg-player",
  "version": "0.1.0",
  "private": true,
  "workspaces": ["functions"],
  "dependencies": {
    "@date-io/luxon": "^1.0.0",
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "axios": "^0.19.2",
    "deep-equals": "^0.0.2",
    "firebase": "^7.14.6",
    "firebase-tools": "^8.4.1",
    "luabundle": "^1.6.0",
    "luxon": "^1.24.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^2.2.0",
    "react-firebaseui": "^4.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "start": "concurrently \"yarn start:react\" \"yarn start:tts\" \"yarn start:functions\" \"yarn start:firebase\" \"yarn start:wiki\"",
    "start:functions": "cd functions && tsc --project tsconfig.json --watch",
    "start:firebase": "firebase emulators:start --only firestore,functions --inspect-functions",
    "start:react": "cross-env BROWSER=none react-scripts start",
    "start:tts": "yarn build:tts && mkdir luaCache || echo && copy \"build\\save.json\" \"%USERPROFILE%\\Documents\\My Games\\Tabletop Simulator\\Saves\\9999.json\" && ts-node --project ttsBuild/tsconfig.json ttsBuild/watchTTS.ts",
    "start:wiki": "yarn exportWiki && wait-port -o silent localhost:8080 && yarn importWiki",
    "build:react": "react-scripts build && rm -rf build/__",
    "build:functions": "cd functions && yarn build",
    "build:tts": "ts-node --project ttsBuild/tsconfig.json ttsBuild/buildTTS.ts",
    "build": "concurrently \"yarn build:react\" \"yarn build:functions\" \"yarn build:tts\"",
    "updateSaveFile": "ts-node --project ttsBuild/tsconfig.json ttsBuild/updateSaveFile.ts",
    "createConfigFile": "ts-node --project ttsBuild/tsconfig.json ttsBuild/createConfigFile.ts",
    "exportWiki": "ts-node --project dbUtil/tsconfig.json dbUtil/exportWiki.ts",
    "importWiki": "ts-node --project dbUtil/tsconfig.json dbUtil/importWiki.ts",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.1",
    "@types/luxon": "^1.24.0",
    "@types/react-router-dom": "^5.1.5",
    "bson": "^4.0.4",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "firestore-export-import": "^0.6.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "ttstk-channels": "^0.2.2",
    "wait-port": "^0.2.9"
  }
}
