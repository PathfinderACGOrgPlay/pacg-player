{
  "compilerOptions": {
    "lib": ["es2018", "DOM"],
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2018",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react"
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
